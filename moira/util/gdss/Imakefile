#define have_subdirs
SUBDIRS = lib

all::

gdss_test: lib/libgdss.a
	$(CC) $(CFLAGS) -I./include -o $@ $@.c lib/libgdss.a $(LLIB) -lkrb -ldes -lhesiod $(OSLIBS)

clean::
	rm -f gdss_test

