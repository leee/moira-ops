SRCS= startdcm.c dcm.c utils.c
CODE=$(SRCS)
OBJS=dcm.o utils.o
#ifdef GDSS
GDSSLIB=../gdss/rgdss.o /mit/gdss/`machtype`/libgdss.a
#endif

