# $Id: Makefile.in,v 1.6 1999-07-12 16:20:59 danw Exp $

SHELL=/bin/sh
@SET_MAKE@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(CPPFLAGS) $(CFLAGS) $(DEFS)
LDFLAGS=@LDFLAGS@
LIBS=../lib/libmrclient.a @LIBS@
MR_LIBDEP=@MR_LIBDEP@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
BUILDTOP=../..
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

OBJS=chfn.o chpobox.o chsh.o

TARGET=chfn chpobox chsh

.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

all: $(TARGET)

clean:
	rm -f $(OBJS) $(TARGET)

cleandir distclean: clean
	rm -f Makefile

depend:

install: all
	$(INSTALL_PROGRAM) chfn $(bindir)
	$(INSTALL_PROGRAM) chpobox $(bindir)
	$(INSTALL_PROGRAM) chsh $(bindir)

chfn: chfn.o ../lib/libmrclient.a $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $@.o $(LIBS)

chpobox: chpobox.o ../lib/libmrclient.a $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $@.o $(LIBS)

chsh: chsh.o ../lib/libmrclient.a $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $@.o $(LIBS)
