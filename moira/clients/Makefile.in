SHELL=/bin/sh
@SET_MAKE@

srcdir=@srcdir@
VPATH=@srcdir@

CURSES_SUBDIRS=mailmaint
SUBDIRS=lib addusr blanche moira mrcheck mrtest passwd @CURSES_SUBDIRS@

all:
	@for d in $(SUBDIRS); do (echo "### Making $@ in clients/$$d"; cd $$d; $(MAKE) $@) || exit 1; done

clean:
	@for d in $(SUBDIRS); do (echo "### Making $@ in clients/$$d"; cd $$d; $(MAKE) $@) || exit 1; done

cleandir distclean:
	rm -f Makefile
	@for d in $(SUBDIRS); do (echo "### Making $@ in clients/$$d"; cd $$d; $(MAKE) $@) || exit 1; done

depend:
	@for d in $(SUBDIRS); do (echo "### Making $@ in clients/$$d"; cd $$d; $(MAKE) $@) || exit 1; done

install:
	@for d in $(SUBDIRS); do (echo "### Making $@ in clients/$$d"; cd $$d; $(MAKE) $@) || exit 1; done
